
public class node {
	
	IMF_member x;
	Point p;
	int dept=0;
	int cost;
	boolean goal;
	int c;
	String transition;
	
	public node() {
		
	}
	
	
	public node(IMF_member x, int depth, int cost, boolean goal, int c, String transition,Point p) {
		
		this.x = x;
		this.cost = cost;
		this.goal = goal;
		this.c = c;
		this.transition = transition;
		this.p=p;
	}


	public IMF_member getX() {
		return x;
	}
	public void setX(IMF_member x) {
		this.x = x;
	}

	public int getCost() {
		return cost;
	}
	public void setCost(int cost) {
		this.cost = cost;
	}
	public boolean isGoal() {
		return goal;
	}
	public void setGoal(boolean goal) {
		this.goal = goal;
	}
	public String getTransition() {
		return transition;
	}
	public void setTransition(String transition) {
		this.transition = transition;
	}
	
	

}
