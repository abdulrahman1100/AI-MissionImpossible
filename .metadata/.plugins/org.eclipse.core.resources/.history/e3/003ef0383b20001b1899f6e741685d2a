
public class search_Tree {
	
	node root;

	
	private node addRecursive(node current) {
		
		if(current==null)
			return new	node();
		
		if(current.transition=="left") {
			current.left=addRecursive(current.left);	
		}else if(current.transition=="right"){
			current.left=addRecursive(current.left);
		}else if(current.transition=="up"){
			current.left=addRecursive(current.left);
		}else if(current.transition=="down"){
			current.left=addRecursive(current.left);
		}else if(current.transition=="carry"){
			current.left=addRecursive(current.left);
		}else if(current.transition=="drop"){
			current.left=addRecursive(current.left);
		}
		
	}

}
